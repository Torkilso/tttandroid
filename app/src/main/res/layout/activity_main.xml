<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.heidu.ticktacktoe.MainActivity"
    android:background="@color/colorPrimary">

    <ImageView
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView25"
        android:layout_width="100dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:clickable="true"
        android:layout_height="128dp" />

    <ImageView
        android:layout_height="128dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView26"
        android:layout_width="100dp"
        android:layout_alignTop="@+id/imageView25"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:clickable="true" />

    <ImageView
        android:layout_height="128dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView29"
        android:layout_width="100dp"
        android:layout_below="@+id/imageView25"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:clickable="true" />

    <ImageView
        android:layout_height="128dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView30"
        android:layout_width="100dp"
        android:clickable="true"
        android:layout_below="@+id/imageView26"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_height="128dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView31"
        android:layout_width="100dp"
        android:clickable="true"
        android:layout_alignTop="@+id/imageView25"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_height="128dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView24"
        android:layout_width="100dp"
        android:clickable="true"
        android:layout_above="@+id/imageView26"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_height="128dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView1"
        android:layout_width="100dp"
        android:clickable="true"
        android:layout_below="@+id/imageView31"
        android:layout_alignLeft="@+id/imageView31"
        android:layout_alignStart="@+id/imageView31" />

    <ImageView
        android:layout_height="128dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView20"
        android:layout_width="100dp"
        android:clickable="true"
        android:layout_above="@+id/imageView25"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_height="128dp"
        app:srcCompat="@drawable/blank"
        android:id="@+id/imageView27"
        android:layout_width="100dp"
        android:clickable="true"
        android:layout_above="@+id/imageView31"
        android:layout_alignLeft="@+id/imageView31"
        android:layout_alignStart="@+id/imageView31" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/header"
        android:editable="false"
        android:background="@android:color/transparent"
        android:clickable="false" />

    <Button
        android:text="Reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/resetBtn"
        android:layout_below="@+id/imageView1"
        android:layout_centerHorizontal="true"
        android:visibility="invisible" />

</RelativeLayout>
